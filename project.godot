config_version=5

[application]

config/name="InteractiveNovel"
config/description="Combat rpg story game made by Glenville Dixon Jr"
config/version="0.4"
run/main_scene="res://scenes/main_menu.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
